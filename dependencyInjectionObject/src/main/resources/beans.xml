<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id = "englishStudent" class = "com.practice.spring.dio.Student">
        <property name="id" value="1"/>
        <property name="subject" ref="subject"/>
    </bean>

    <bean id = "mathematicsStudent" class = "com.practice.spring.dio.Student">
        <property name="id" value="2"/>
        <property name="subject" ref="subject"/>
    </bean>

    <bean id = "subject" class = "com.practice.spring.dio.Subject"/>

</beans>